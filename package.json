  "license": "ISC",
  "standard": {
    "ignore": [
      "dist/"
    ]
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "debug": "^2.6.0",
    "fs-extra": "^2.0.0",
    "object-path": "^0.11.3",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "standard": "^10.0.1"
  }
