  "main": "index.js",
  "scripts": {
    "preversion": "npm run lint && npm run test && npm run babel:transpile && git add dist/* && (git commit -m \"transpile\" || echo \"skipping transpile commit\")",
    "babel:transpile": "rm -rf dist/* && babel --no-comments src -d dist",
    "babel:watch": "npm run babel:transpile -- --watch",
    "lint": "standard \"src/**/*.js\"",
    "test": "mocha --require test/helpers/babel-register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daerion/octomore.git"
  },
